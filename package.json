    "browser-sync": "^2.27.7",
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0"
    "js-yaml": "^4.1.0",
    "luxon": "^2.3.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "autoprefixer": "^10.4.2"
  }
